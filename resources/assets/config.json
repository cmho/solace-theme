{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ],
    "sw": [
      "./scripts/sw.js"
    ]
  },
  "publicPath": "/wp-content/themes/solace-theme",
  "devUrl": "http://solacelarp.loc",
  "proxyUrl": "http://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php",
    "resources/assets/**/*.js",
    "resources/assets/**/*.scss",
    "resources/assets/**/*.jpg",
    "resources/assets/**/*.png"
  ]
}
